#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'contrail-api-server==0.1dev','console_scripts','contrail-api'
__requires__ = 'contrail-api-server==0.1dev'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('contrail-api-server==0.1dev', 'console_scripts', 'contrail-api')()
    )
